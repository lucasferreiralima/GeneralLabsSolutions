@model GeneralLabSolutions.Domain.Entities.Produto

@{
    ViewData["Title"] = "Excluir Produto";
}

<h3>Excluir Produto</h3>

<h3>Tem certeza que deseja excluir este registro?</h3>
<div>
    <hr />
     <partial name="_details_e_delete" model="Model" />
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Excluir" class="btn btn-outline-danger" />
        <a asp-action="Index" class="btn btn-outline-success">Voltar à Lista</a>
    </form>
</div>
