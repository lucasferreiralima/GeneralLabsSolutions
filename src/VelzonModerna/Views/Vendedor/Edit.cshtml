@model GeneralLabSolutions.Domain.Entities.Vendedor

@{
    ViewData["Title"] = "Editar Vendedor";
}

<h3>Editar Vendedor</h3>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="All" class="text-danger"></div>

            <partial name="_edit_e_create" />

            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <input type="submit" value="Editar" class="btn btn-outline-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" class="btn btn-outline-success">Listagem</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
